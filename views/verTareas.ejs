<%- include('templates/head.ejs') %>

<div class="field is-static">
  <h1 class="misTareas">Mis tareas</h1>

  <div class="columns">
    <div class="column">
<table class="table">
    <thead>
      <tr>
        <th><abbr title="Proyecto">Proyecto</abbr></th>
        <th><abbr title="Tarea">Tarea</abbr></th>
        <th><abbr title="Fecha">Fecha</abbr></th>
        <th><abbr title="Horas">Horas</abbr></th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <% for (let e of empleados) { %>
          <% for (let t of tarea) { %>
          <% if (e.idEmpleados = e.nombre) { %> 
              <td><%= t.nombreT %></td>
          <% } %>
      <% } %>
      <% } %>
      </tr>
      </tbody>
    </div>
  </div>