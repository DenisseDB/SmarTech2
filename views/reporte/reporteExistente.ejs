<div class="titulo">Reporte semanal NatDev</div>

            <div class="fechas_reporte">
                <div class = "row">
                    <div class="input-field is-normal">
                        <h6>Fecha inicio: 
                        <%= reporteN[0].fechaInicio %>
                        </h6>
                        <h6>Fecha fin: 
                        <%= reporteN[0].fechaFin %>
                        </h6>
                        <h6>Número reporte: 
                        <%= reporteN[0].noReporte %>
                        </h6>
                    </div>
                </div>
            </div>

            <div class="columns">
                <div class="column">
                    <div class="porcentajes">
                        <h6>Porcentaje: </h6>
                        <input class = "porcentaje" type = "number" id = "porcentaje"
                        onkeyup="eficiencia()" onclick="eficiencia()" placeholder="0.85" value = "0.85">
                        <h6>Verdadero porcentaje de eficiencia: <%= reporteN[0].eficiencia %></h6> 
                        <h6>Proporción: <%= reporteN[0].proporcion %></h6> 
                        
                    </div>
                </div>
            </div>
                
    <div class="columns">
        <div class="column">
            <div class="inputs_reporte">
                <div class = "field is-horizontal"> 
                    <label class = "label">Personal de Medio Tiempo</label>
                    <input class = "input" type = "number" min="0" placeholder = "Ingresar" 
                    onkeyup="horasMedioT()" onclick="horasMedioT()"
                    id = "personalMedioT" value = "0">   
                    <label class = "label">Horas: <%= reporteN[0].horasMedioT %></label>                             
                </div>

                <div class="field is-horizontal">
                    <label class = "label">Personal de Tiempo Completo</label>
                    <input class = "input" type = "number" min="0" placeholder = "Ingresar" 
                    onkeyup="horasCompletoT()" onclick="horasCompletoT()" 
                    id = "personalCompletoT" value = "0">
                    <label class = "label">Horas</label>  
                    <div class = "horasCompletoT" type = "number" id= "horasCompletoT">0</div>                      
                </div>
                <div class="field is-horizontal">
                    <label class = "label">Total de personal</label>
                    <div type = "number" id= "totalPersonal" min = "0" value = "0">0</div> 
                </div>
                <div class="field is-horizontal">
                    <label class = "label">Horas Esperadas</label>
                    <div type = "number" id= "horasEsperadas" min = "0" value = "0">0</div>
                </div>
                <div class="field is-horizontal">
                    <label class = "label">Horas de Vacaciones</label>
                    <input class = "input" type = "number" min = "0"
                    onkeyup="horasEsperadas()" onclick="horasEsperadas()"
                    value = "0" id= "horasVacaciones">  
                </div>
                <div class="field is-horizontal">
                    <label class = "label">Descripcion</label>
                    <input id = "descripcion" type ="text" placeholder="Ingresar">
                </div>
            </div>
        </div>
    </div>

    <table id="tabla_reporte">
        <thead>
        <tr>
            <th><abbr title="Proyecto">Proyecto</abbr></th>
            <th><abbr title="Tarea">Horas registradas</abbr></th>
            <th><abbr title="Horas">Horas reales</abbr></th>
        </tr>
        </thead>

        
        <tbody id="datos_tabla"> 
            <script>
                const nombreP = new Array();
            </script>       
            <% for (let h of horas) { %>
                <tr>
                    <td><%= h.nombreP %></td> 
                    <td><input id = "horasTrabajo['<%= h.nombreP %>']" type = "number" value="<%= h.horasTrabajo %>" readonly></td> 
                    <td><input value = "<%= h.horasTrabajo %>" type="number" id = "horasReales['<%= h.nombreP %>']" min = "0" placeholder="0" onkeyup="sumHoras()" onclick="sumHoras()"></td>
                </tr>
                <script>
                    nombreP.push("<%= h.nombreP %>");
                </script>
             <% } %>
            <tr>
                <td><b>TOTAL</b></td>
                <td id = "totalHorasTrabajo" type = "number" value = "0"></td>
                <td id = "totalHorasReales" type = "number"></td>
            </tr>
        </tbody>

        <!--Botón de guardar-->
        <input type="submit" id = "reportebtn" value = "Guardar" onclick="postNuevoReporte()">

<script>
    $(document).ready( function () {
  $('#tabla_reporte').DataTable();
    } );
  </script>

<script src="/js/scriptReporte.js"></script>